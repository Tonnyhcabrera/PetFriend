<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:onClick="CambiarVentanaListaVacunas"
    tools:context=".PantallaPrincipal">


    <Button
        android:id="@+id/pantallaListaMascotas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="152dp"
        android:onClick="CambiarVentanaListaMascotas"
        android:text="Lista Mascotas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/PantallaListaVacunas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="CambiarVentanaListaVacunas"
        android:text="Lista Vacunas"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pantallaListaMascotas" />


</androidx.constraintlayout.widget.ConstraintLayout>